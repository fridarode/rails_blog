<h1>Add Post</h1>
<p>form for user to create new blog post</p>

<%=form_for :post, url:{action: "create"}, html:{class:"newform"} do |f| %>
<p>
<%=f.text_field :title, placeholder: 'Title'%><br>
<%=f.text_area :content, placeholder: 'Content'%><br>
<%=f.submit({:class => "btn btn-primary"})%>
</p>
<%end%>
